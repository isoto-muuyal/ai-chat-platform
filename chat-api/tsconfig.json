{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
    "include": ["src/**/*.ts", "routes/**/*.ts", "config/**/*.ts"],
    "exclude": ["node_modules", "dist", "test"]

} 
